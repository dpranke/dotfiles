#!/bin/bash
rsync -av \
      --exclude=.bash_history \
      --exclude=.dropbox/ \
      --exclude=.ssh/ \
      --exclude=.viminfo \
      --exclude=.Trash/ \
      --exclude=Dropbox/ \
      --exclude=Downloads/ \
      --exclude=Library/ \
      $*

